{
  "expo": {
    "name": "vibeventz-app",
    "slug": "vibeventz-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "${GOOGLE_MAPS_API_KEY}"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Vibeventz needs camera access so you can capture event and profile photos.",
        "NSPhotoLibraryUsageDescription": "Vibeventz needs access to your photo library so you can upload event and profile images.",
        "NSLocationWhenInUseUsageDescription": "Vibeventz uses your location to show you relevant events and vendors near you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "${GOOGLE_MAPS_API_KEY}"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.vibeventzapp"
    },
    "scheme": "vibeventz",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "536d0059-dd1a-452c-b92a-67e2b2da6d31"
      }
    },
    "owner": "sifosman"
  }
}
